{ 
  "compilerOptions":{ 
     "target":"es6",
     "moduleResolution":"node",
     "module":"commonjs",
     "declaration":false,
     "noLib":false,
     "emitDecoratorMetadata":true,
     "sourceMap":true,
     "pretty":true,
     "allowUnreachableCode":true,
     "allowUnusedLabels":true,
     "noImplicitAny":true,
     "noImplicitReturns":false,
     "noImplicitUseStrict":false,
     "experimentalDecorators": true,
     "allowJs": true,
     "outDir": "./dist/out-tsc",
     "baseUrl":"./",
     "importHelpers": true,
     "listFiles":false,
     "noEmitHelpers":true
  },
  "include":[ 
    "src/**/*.d.ts"
  ],
  "files": [
    "src/main.ts",
        "src/polyfills.ts" 
  ],
  "exclude":[ 
     "node_modules"
  ],
  "compileOnSave":false
}   